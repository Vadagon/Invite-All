{
    "name": "Invite post likers to like page for Facebookâ„¢",
    "description": "Mass invite all your friends and everyone who liked your post to like your Facebook Pages or Events with one click.",
    "manifest_version": 2,
    "version": "1",
	"icons": { 
		"128": "icons/icon128.png"
	},
    "browser_action": {
        "default_icon": "icons/icon128.png",
        "default_title": "Go to Facebook.com"
  },
    "permissions": [
        "tabs",
        "notifications",
        "*://*.facebook.com/*"
    ],
    "content_scripts": [{
        "matches": ["*://*.facebook.com/*"],
        "js": [
          "plugins/jquery.min.js",
          "plugins/rx.lite.min.js",
          "scripts/contents.js"
        ]
    }],
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    }
}


